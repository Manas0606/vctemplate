{
  "@context": [
    "https://www.w3.org/ns/credentials/v2",
    "https://manas0606.github.io/vctemplate/health.json",
    "https://w3id.org/security/suites/ed25519-2020/v1"
  ],
  "issuer": "${_issuer}",
  "type": [
    "VerifiableCredential",
    "HealthCredential"
  ],
  "validFrom": "${validFrom}",
  "validUntil": "${validUntil}",
  "credentialSubject": {
    "id": "${_holderId}",
    "fullName": "${fullName}",
    "gender": "${gender}",
    "dateOfBirth": "${dateOfBirth}",
    "healthId": "${healthId}",
    "bloodGroup": "${bloodGroup}",
    "allergies": "${allergies}",
    "medicalHistory": "${medicalHistory}"
  }
}
